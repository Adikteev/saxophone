{
  "name": "saxophone",
  "description": "Fast and lightweight event-driven XML parser in pure JavaScript",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/matteodelabre/saxophone.git"
  },
  "scripts": {
    "build": "babel index.js --out-dir dist",
    "lint": "eslint .",
    "pretest": "npm run build",
    "test": "tape test | tap-spec && npm run lint",
    "precoverage": "npm run build",
    "coverage": "istanbul cover -- tape test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "prebenchmark": "npm run build",
    "benchmark": "node benchmark",
    "prepublish": "npm run build"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=4.4.0"
  },
  "keywords": [
    "xml",
    "parser",
    "sax",
    "fast"
  ],
  "author": {
    "name": "Mattéo Delabre ✏️",
    "email": "contact@matteodelabre.me",
    "url": "https://github.com/matteodelabre"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "benchmark": "^2.1.1",
    "common-tags": "^1.3.1",
    "coveralls": "^2.11.12",
    "eslint": "^3.2.2",
    "istanbul": "^0.4.4",
    "lodash.uniq": "^4.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  }
}
